<!--pages/detail/detail.wxml-->

<view class="container">
   <text>{{newsId}}-{{authorName}}</text>
   <view class='shopcart'>
     <button type='primary' size='mini' bindtap='goCart' data-newsid='{{newsId}}'>购物车</button>
     <button type='primary' size='mini' bindtap='addCart' data-newsid='{{newsId}}'>加入购物车</button>
  </view> 

  
</view>